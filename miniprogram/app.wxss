@import "./iconfont/iconfont.wxss";
page {
  background-color: #fefefe;
}
.back-icon {
  width: 50rpx;
  height: 50rpx;
}
.green {
  color: green !important;
}
.blue {
  color: #3a81f6 !important;
}

/* 动画 */
@keyframes scaleIn {
  0% {
    opacity: 0;
    transform: scale(1.1);
  }
  100% {
    opacity: 1;
    transform: scale(1);
  }
}