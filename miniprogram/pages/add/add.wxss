.inpt {
  display: flex;
  align-items: center;
  padding: 30rpx 40rpx;
}
.title {
  font-size: 38rpx;
  color: #878b97;
  padding-right: 20rpx;
}
.inpt input {
  background-color: #ebecf5;
  flex: 1;
  padding: 20rpx;
  color: #040819;
  border-radius: 6rpx;
}
.img {
  padding: 30rpx 40rpx;
}
.img-wrap {
  display: flex;
  flex-wrap: wrap;
  padding-top: 20rpx;
}
.item {
  flex: none;
  width: 200rpx;
  height: 200rpx;
  background-color: #fefefe;
  margin: 12rpx;
  overflow: hidden
}
.item image {
  display: block;
  width: 200rpx;
  height: 200rpx;
}
.upload {
  flex: none;
  width: 200rpx;
  height: 200rpx;
  margin: 12rpx;
  font-size: 100rpx;
  line-height: 200rpx;
  text-align: center;
  color: #878b97;
  border: 1rpx dashed #ebecf5;
}
.btn {
  color: #fefefe;
  text-align: center;
  background-color: #5bc7f6;
  margin: 80rpx 40rpx 0;
  border-radius: 8rpx;
  line-height: 88rpx;
}
.disabled {
  background-color: #ccc;
}